	  </columns>
	  <columns id="STREET">
		  <datatypeSQL xsi:type="DataTypes:CharacterStringSQLDataType" id="NVARCHAR" length="60"/>
	  </columns>
	  <columns id="ADDRESSID">
		  <datatypeSQL xsi:type="DataTypes:CharacterStringSQLDataType" id="NVARCHAR" length="10"/>
	  </columns>
		<schema id="_SYS_BIC"/>
	</type>
</parameters>
